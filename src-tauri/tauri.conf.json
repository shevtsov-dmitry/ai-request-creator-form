{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "ai-request-creator-form",
    "version": "0.1.0",
    "identifier": "com.shd.ai-request-creator-form",
    "build": {
        "beforeDevCommand": "bun run dev",
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": "bun run build",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "title": "ai-request-creator-form",
                "width": 800,
                "height": 600
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/16x16.png",
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/256x256.png",
            "icons/512x512.png",
            "icons/1024x1024.png",
            "icons/icon.png",
            "icons/icon.ico"
        ]
    }
}
